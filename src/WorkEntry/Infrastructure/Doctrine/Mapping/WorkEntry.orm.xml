<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="App\WorkEntry\Domain\Entity\WorkEntry" table="work_entries">

        <id name="id" type="work_entry_id" column="id"/>

        <field name="userId" type="user_id" column="user_id"/>

        <field name="startDate" type="datetime_immutable" column="start_date"/>
        <field name="endDate" type="datetime_immutable" column="end_date"/>

        <field name="createdAt" type="datetime_immutable" column="created_at"/>
        <field name="updatedAt" type="datetime_immutable" column="updated_at"/>
        <field name="deletedAt" type="datetime_immutable" column="deleted_at" nullable="true"/>

    </entity>
</doctrine-mapping>
